{
  "extends": ["next", "prettier"],
  "plugins": ["@typescript-eslint", "prettier"],
  "settings": {
    "next": {
      "rootDir": "apps/next/"
    }
  },
  "root": true,
  "rules": {
    "jsx-a11y/alt-text": "off",
    "react/no-unescaped-entities": "off",
    "import/no-anonymous-default-export": "off"
  }
}
